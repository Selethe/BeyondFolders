{
    "manifest_version": 3,
    "name": "Beyond Folders",
    "permissions": ["storage"],
    "description": "Adding folders to the charachter creator from DnDbeyond",
    "version": "1.0",
    "icons": {
      "16": "beyondlogo16.png",
      "48": "beyondlogo48.png",
      "128": "beyondlogo128.png"
    },
    "action: {
      "default_popup": "popup.html"
    },
   "content_scripts": [{
      "matches": ["https://www.dndbeyond.com/characters"],
      "css": ["main.css"],
      "js": ["content-script.js"]
   }]
  }
